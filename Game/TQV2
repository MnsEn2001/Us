local function NotifyUser(title, text)
    game.StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = 5
    })
end

local function Run_Script()
    NotifyUser("Game", "Treasure Quest : V2")
end

local function CheckKeyRun()
    if _G.key_run then
        NotifyUserCheck("Check", "✅ Your key is correct. ✅")
        wait(3)
        Run_Script()
        return true
    else
        NotifyUserCheck("Check", "❌ Your key is invalid. ❌")
        return false
    end
end

CheckKeyRun()
